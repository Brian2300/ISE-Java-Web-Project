<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>ISE</display-name>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>RetrieveImg</servlet-name>
		<servlet-class>servlets.RetrieveImg</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RetrieveImg</servlet-name>
		<url-pattern>/retrieveImg</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Authcalendar</servlet-name>
		<servlet-class>src.servlets.Authcalendar</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Authcalendar</servlet-name>
		<url-pattern>/authcalendar</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Calendarjson</servlet-name>
		<servlet-class>src.servlets.Calendarjson</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Calendarjson</servlet-name>
		<url-pattern>/calendarjson</url-pattern>
	</servlet-mapping>
</web-app>